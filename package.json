{
  "name": "@neofinancial/pr-title-action",
  "version": "1.0.0",
  "private": true,
  "description": "Verify that your pull request titles start with a ticket ID",
  "main": "lib/main.js",
  "scripts": {
    "build": "NODE_ENV=production ncc build src/main.ts -o build --minify",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint \"**/*.{ts,js}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neofinancial/pr-title-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Neo Financial Engineering <engineering@neofinancial.com>",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "^2.1.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^6.8.0",
    "eslint-config-neo": "^0.5.2",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  },
  "license": "MIT"
}
